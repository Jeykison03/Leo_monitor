{
    "name": "Leo Monitor - NO_FEATURES_MODE",
    "image": "mcr.microsoft.com/devcontainers/universal:2",
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer",
                "tamasfe.even-better-toml",
                "serayuzgur.crates",
                "vue.volar",
                "dbaeumer.vscode-eslint"
            ]
        }
    },
    "forwardPorts": [
        8080,
        5173
    ],
    "postCreateCommand": "cd backend && cargo --version",
    "remoteUser": "codespace"
}